{
  "roots": [
    "<rootDir>"
  ],
  "testMatch": [
    "<rootDir>/tests/**/?(*.)+(spec|test).ts"
  ],
  "transform": {
    "^.+\\.ts": "ts-jest"
  },
  "snapshotResolver": "./tests/snapshotResolver.js",
  "setupFilesAfterEnv": [
  ],
  "globals": {
    "ts-jest": {
      "tsconfig": "./tsconfig.jest.json"
    }
  },
  "slowTestThreshold": 20,
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/tests/"
  ]
}
